{
  "version": "http://to_be_decided.com/draft-1/schema#",
  "display": "Elderly Diabetes Patients",
  "cohortDefinition": {
    "version": "http://to_be_decided.com/draft-1/schema#",
    "display": "Patients above 65 with diabetes",
    "inclusionCriteria": [
      [
        {
          "termCodes": [
            {
              "code": "424144002",
              "system": "http://snomed.info/sct",
              "display": "Current chronological age"
            }
          ],
          "context": {
            "code": "Patient",
            "system": "fdpg.mii.cds",
            "version": "1.0.0",
            "display": "Patient"
          },
          "valueFilter": {
            "type": "quantity-comparator",
            "unit": {
              "code": "a",
              "display": "years"
            },
            "value": 65,
            "comparator": "gt"
          }
        }
      ],
      [
        {
          "termCodes": [
            {
              "code": "44054006",
              "system": "http://snomed.info/sct",
              "display": "Diabetes mellitus"
            }
          ],
          "context": {
            "code": "Diagnosis",
            "system": "fdpg.mii.cds",
            "version": "1.0.0",
            "display": "Diagnosis"
          }
        }
      ]
    ]
  },
  "dataExtraction": {
    "attributeGroups": [
      {
        "groupReference": "Medication",
        "attributes": [
          {
            "attributeRef": "medicationCode",
            "mustHave": true
          },
          {
            "attributeRef": "dosage",
            "mustHave": true
          }
        ],
        "filter": [
          [
            {
              "termCodes": [
                {
                  "code": "A10",
                  "system": "http://fhir.de/CodeSystem/bfarm/atc",
                  "version": "2022",
                  "display": "Drugs used in diabetes"
                }
              ],
              "context": {
                "code": "Medication",
                "system": "fdpg.mii.cds",
                "version": "1.0.0",
                "display": "Medication"
              }
            }
          ]
        ]
      }
    ]
  }
}
